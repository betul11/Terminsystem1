<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.terminsystem1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Terminsystem1"
        android:windowSoftInputMode="adjustNothing">
        <activity android:name=".dateSendingActivity"></activity>
        <activity android:name=".academicLoginActivity" />
        <activity android:name=".studentLoginActivity" />
        <activity android:name=".studentMyAppointmentsActivity" />
        <activity android:name=".studentAppointmentActivity" />
        <activity android:name=".StudentHomeScreenActivity" />
        <activity android:name=".AcademicHomeScreenActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>